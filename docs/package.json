    "dev": "vite",
    "build": "vite build",
    "pdf": "node scripts/capture-pdf.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shiki": "^0.14.2"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^2.3.0",
    "@types/mdx": "^2.0.5",
    "@types/node": "^18.16.3",
    "@types/react-dom": "^18.2.3",
    "@vitejs/plugin-react": "^4.0.0",
    "patch-package": "^7.0.0",
    "puppeteer": "^20.1.0",
    "rehype-slug": "^5.1.0",
    "sass": "^1.62.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.4",
    "vite-plugin-static-copy": "^0.14.0"
  }
}

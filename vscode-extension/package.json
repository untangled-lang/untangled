{
  "name": "untangled",
  "displayName": "Untangled",
  "description": "VSCode syntax highlighting for the Untangled language.",
  "version": "0.0.1",
  "publisher": "cs107",
  "repository": {
    "type": "git",
    "url": "https://github.com/untangled-lang/untangled",
    "directory": "vscode-extension"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "untangled",
      "aliases": ["Untangled", "untangled"],
      "extensions": [".unt"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "untangled",
      "scopeName": "source.untangled",
      "path": "./syntaxes/untangled.tmLanguage.json"
    }]
  }
}

thread_def Child {
  receive {
    semaphore s -> {
      parent << "Received semaphore\n";
      s--;
      s++;
    }
    _ -> {}
  }
  parent << "done";
}

thread_def Main {
  semaphore s = make_semaphore(2);
  thread child = spawn Child;
  child << s;

  receive {
    string x -> print(x);
    _ -> {}
  }

  receive {
    _ -> {
      print("Done\n");
    }
  }
}
